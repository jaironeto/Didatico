				"        this._nome = novoNome;",
				"    }",
				"",
				"    // Getter para a idade",
				"    get idade(): number {",
				"        return this._idade;",
				"    }",
				"",
				"    // Setter para a idade",
				"    set idade(novaIdade: number) {",
				"        this._idade = novaIdade;",
				"    }",
				"}",
				"",
				"// Exemplo de uso",
				"let pessoa = new Pessoa(“João“, 30);",
