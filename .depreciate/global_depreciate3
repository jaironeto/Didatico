				"class Pessoa {",
				"    private _nome: string;",
				"    private _idade: number;",
				"",
				"    constructor(nome: string, idade: number) {",
				"        this._nome = nome;",
				"        this._idade = idade;",
				"    }",
				"",
				"    // Getter para o nome",
				"    get nome(): string {",
				"        return this._nome;",
				"    }",
				"",
				"    // Setter para o nome",
